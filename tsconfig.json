{
  "compilerOptions": {
    "target": "ESNext",
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "module": "ES6",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "exclude": ["node_modules", "public"],
  "include": ["src"]
}
